{
  "name": "treeize",
  "version": "2.1.2",
  "description": "Converts tabular row data (as from SQL joins, flat JSON, etc) to deep object graphs based on simple column naming conventions - without the use of an ORM or models.",
  "main": "./lib/treeize.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kwhitley/treeize.git"
  },
  "keywords": [
    "JSON",
    "SQL",
    "CSV",
    "excel",
    "tree",
    "object",
    "graph",
    "hydration",
    "incongrous",
    "multi-source",
    "model",
    "deep",
    "convert",
    "expand",
    "flat",
    "array",
    "ORM"
  ],
  "author": "Kevin R. Whitley <kevin3503@gmail.com> (http://krwhitley.com/)",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://en.wikipedia.org/wiki/MIT_License"
    }
  ],
  "bugs": {
    "url": "https://github.com/kwhitley/treeize/issues"
  },
  "dependencies": {
    "inflection": "^1.12.0",
    "lodash": "^4.17.4",
    "object-merge": "^2.5.1"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "mocha": "latest",
    "nodemon": "^1.11.0",
    "should": "latest"
  },
  "scripts": {
    "test": "mocha",
    "test:watch": "mocha --watch test/test.js .",
    "benchmark": "npm run test:watch & nodemon --watch . benchmark/index.js"
  }
}
